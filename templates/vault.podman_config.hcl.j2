#
ui = true
api_addr = "https://127.0.0.1:8200"
disable_mlock = true
default_lease_ttl = "168h"
max_lease_ttl = "720h"

storage "file" {
  path    = "/vault/file"
}

listener "tcp" {
  address     = "0.0.0.0:8200"
  tls_disable = "false"
  tls_cert_file   = "/vault/config/public.crt"
  tls_key_file    = "/vault/config/private.key"
}